{{#if errorMessage}}
    <div class="alert alert-danger error-container">
        <div class="error-title">{{fa-icon "exclamation-circle"}} Error Loading Details </div>
        {{errorMessage}}
    </div>
{{/if}}
{{#if details.link}}
    <div class="p-title">
        <a class="p-link" href={{details.link}}>
            Search Information in Hyas
            {{fa-icon "external-link-square" class="external-link-icon"}}
        </a>
    </div>
{{/if}}

<ul class="nav nav-tabs">
    {{#if (eq block.entity.type "IPv4")}}
        <li class="nav-item">
            <a {{action "changeTab" "deviceGeo"}} class="nav-link {{if (eq activeTab "deviceGeo") "active"}}" href="#">Device Geo</a>
        </li>
        <li class="nav-item">
            <a {{action "changeTab" "ipdynamic"}} class="nav-link {{if (eq activeTab "ipDynamic") "active"}}" href="#">Dynamic DNS</a>
        </li>
        <li class="nav-item">
            <a {{action "changeTab" "passivednsip"}} class="nav-link {{if (eq activeTab "ippassivednsip") "active"}}" href="#">Passive DNS</a>
        </li>
        <li class="nav-item">
            <a {{action "changeTab" "ipsample"}} class="nav-link {{if (eq activeTab "ipsample") "active"}}" href="#">Associated Hashes</a>
        </li>
    {{/if}}
    {{#if (eq block.entity.type "IPv6")}}
        <li class="nav-item">
            <a {{action "changeTab" "deviceGeov6"}} class="nav-link {{if (eq activeTab "deviceGeov6") "active"}}" href="#">Device Geo</a>
        </li>
    {{/if}}


    {{#if (eq block.entity.type "md5")}}
        <li class="nav-item">
            <a {{action "changeTab" "sample"}} class="nav-link {{if block.isLoadingDetails "disabled"}} {{if (or (eq activeTab "sample")(not activeTab)) "active"}}" href="#">
                Sample {{#if block.isLoadingDetails}}{{fa-icon "spinner-third" fixedWidth=true spin=true class="loading-icon"}}{{/if}}
            </a>
        </li>
    {{/if}}
    {{#if (eq block.entity.type "domain")}}
        <li class="nav-item">
            <a {{action "changeTab" "whois"}} class="nav-link {{if block.isLoadingDetails "disabled"}} {{if (or (eq activeTab "whois")(not activeTab)) "active"}}" href="#">
                WhoIs {{#if block.isLoadingDetails}}{{fa-icon "spinner-third" fixedWidth=true spin=true class="loading-icon"}}{{/if}}
            </a>
        </li>
        <li class="nav-item">
            <a {{action "changeTab" "passivedns"}} class="nav-link {{if (or (eq activeTab "passivedns")(not activeTab)) "active"}}" href="#">
                Passive DNS 
            </a>
        </li>
        <li class="nav-item">
            <a {{action "changeTab" "sslcert"}} class="nav-link {{if block.isLoadingDetails "disabled"}} {{if (or (eq activeTab "sslcert")(not activeTab)) "active"}}" href="#">
                SSL Certificate {{#if block.isLoadingDetails}}{{fa-icon "spinner-third" fixedWidth=true spin=true class="loading-icon"}}{{/if}}
            </a>
        </li>
        <li class="nav-item">
            <a {{action "changeTab" "domainsample"}} class="nav-link {{if block.isLoadingDetails "disabled"}} {{if (or (eq activeTab "domainsample")(not activeTab)) "active"}}" href="#">
                Associated Hash Information {{#if block.isLoadingDetails}}{{fa-icon "spinner-third" fixedWidth=true spin=true class="loading-icon"}}{{/if}}
            </a>
        </li>
    {{/if}}
    {{#if (eq block.entity.type "email")}}
        <li class="nav-item">
            <a {{action "changeTab" "whoisemail"}} class="nav-link {{if block.isLoadingDetails "disabled"}} {{if (or (eq activeTab "whoisemail")(not activeTab)) "active"}}" href="#">
                WhoIs {{#if block.isLoadingDetails}}{{fa-icon "spinner-third" fixedWidth=true spin=true class="loading-icon"}}{{/if}}
            </a>
        </li>
    {{/if}}
</ul>

{{#if (eq activeTab "passivedns")}}
    {{#if details.domainPassive.length}}
        <h1 class="p-title">
            {{fa-icon "globe" fixedWidth=true}} 
        </h1>
        {{#each details.domainPassive as | result |}}
            <div class="mb-2">
                {{#if result.cert_name}}
                    <div>
                        <span class="p-key">Cert Name: </span>
                        <span class="p-value">{{result.cert_name}}</span>
                    </div>
                {{/if}}
                {{#if result.domain}}
                    <div>
                        <span class="p-key">Domain: </span>
                        <span class="p-value">{{result.domain}}</span>
                    </div>
                {{/if}}
                {{#if result.first_seen}}
                    <div>
                        <span class="p-key">First Seen: </span>
                        <span class="p-value">{{result.first_seen}} </span>
                    </div>
                {{/if}}
                {{#if result.last_seen}}
                    <div>
                        <span class="p-key">Last Seen: </span>
                        <span class="p-value">{{result.last_seen}} </span>
                    </div>
                {{/if}}
                {{#if result.ip.geo.country_name}}
                    <div>
                        <span class="p-key">Country Name: </span>
                        <span class="p-value">{{result.ip.geo.country_name}} </span>
                    </div>
                {{/if}}
                {{#if result.ip.geo.city_name}}
                    <div>
                        <span class="p-key">City Name: </span>
                        <span class="p-value">{{result.ip.geo.city_name}} </span>
                    </div>
                {{/if}}
                {{#if result.ip.geo.isp.autonomous_system_organization}}
