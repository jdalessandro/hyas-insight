                    {{#if device.sha256}}
                        <div>
                            <span class="p-key">SHA256: </span>
                            <span class="p-value">{{device.sha256}}</span>
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        {{else}}
            <div class="alert alert-info mt-2" role="alert">
                No Associated Hash Details Found
            </div>
        {{/if}}
    {{/if}}
{{/if}}

{{#if (eq activeTab "sslcert")}}
    {{#if block.isLoadingDetails}}
        <div class="alert alert-info" role="alert">
            SSL Certificate Information is Loading
        </div>
    {{else}}
        {{#if details.domainSsl.related.length}}
            <h1 class="p-title">
                {{fa-icon "certificate" fixedWidth=true}}  SSL Certificate Details
            </h1>
            <div class="mb-2">
                {{#if details.domainSsl.ssl_certs}}
                {{#each details.domainSsl.ssl_certs as | cert |}}
                {{#if cert.ip}}
                    <div>
                        <span class="p-key">Cert IP: </span>
                        <span class="p-value">{{cert.ip}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.cert_key}}
                    <div>
                        <span class="p-key">Cert Key: </span>
                        <span class="p-value">{{cert.ssl_cert.cert_key}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.expire_date}}
                    <div>
                        <span class="p-key">Cert Expiration: </span>
                        <span class="p-value">{{cert.ssl_cert.expire_date}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.issue_date}}
                    <div>
                        <span class="p-key">Cert Issue Date: </span>
                        <span class="p-value">{{cert.ssl_cert.issue_date}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.issuer_commonName}}
                    <div>
                        <span class="p-key">Cert Issue Name: </span>
                        <span class="p-value">{{cert.ssl_cert.issuer_commonName}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.issuer_countryName}}
                    <div>
                        <span class="p-key">Cert Issue Country: </span>
                        <span class="p-value">{{cert.ssl_cert.issuer_countryName}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.issuer_organizationName}}
                    <div>
                        <span class="p-key">Cert Issue Org Name: </span>
                        <span class="p-value">{{cert.ssl_cert.issuer_organizationName}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.subject_commonName}}
                    <div>
                        <span class="p-key">Cert Subject Common Name: </span>
                        <span class="p-value">{{cert.ssl_cert.subject_commonName}}</span>
                    </div>
                {{/if}}
                {{#if cert.ssl_cert.subject_organizationName}}
                    <div>
                        <span class="p-key">Cert Subject Org Name: </span>
                        <span class="p-value">{{cert.ssl_cert.subject_organizationName}}</span>
                    </div>
                {{/if}}
              {{/each}}
              {{/if}}
            </div>
        {{else}}
            <div class="alert alert-info mt-2" role="alert">
                No SSl Certificate Information Found
            </div>
        {{/if}}
    {{/if}}
{{/if}}



